{
  "name": "web-txt2img",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "description": "Browser-only text-to-image generation library (scaffold)",
  "license": "MIT",
  "scripts": {
    "build": "tsc -p .",
    "typecheck": "tsc -p . --noEmit",
    "clean": "rm -rf dist",
    "prepare:ort-assets": "node scripts/copy-ort-assets.cjs",
    "predev": "npm run prepare:ort-assets",
    "postinstall": "npm run prepare:ort-assets",
    "dev": "vite"
  },
  "dependencies": {
    "@huggingface/transformers": "^3.7.1",
    "onnxruntime-web": "^1.18.0",
    "@xenova/transformers": "latest"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vite": "^5.4.0"
  }
}
