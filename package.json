{
  "name": "web-txt2img-workspace",
  "version": "0.0.0",
  "private": true,
  "description": "Workspace for web-txt2img (library + examples)",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build:lib": "npm run -w packages/web-txt2img build",
    "typecheck": "npm run -w packages/web-txt2img typecheck",
    "clean": "rm -rf packages/web-txt2img/dist examples/vanilla-worker/dist",
    "dev:vanilla": "npm run build:lib && npm run -w examples/vanilla-worker dev",
    "build:vanilla": "npm run build:lib && npm run -w examples/vanilla-worker build"
  }
}
